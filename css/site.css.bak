	border-radius: 20px;
}

nav .opcao:hover {
    background-color: #947551;
}
.quadrado {
	position: relative;
	width: 800px; 
    height: 250px; 
    background-color: #a5d6a7;
    margin: 210px auto; 
    display: flex; 
    align-items: center; 
    justify-content: center; 
    text-align: center; 
    border-radius: 20px; 
    padding: 10px; 
}
.cesto {
	position: absolute;
    top: -10px; 
    left: 3.5%; 
    transform: translateX(-70%) rotate(0deg); 
    width: 345px; 
    height: 345px; 
}
 .botao h2 {
